<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://ivyrep.jayasoft.org/ivy-doc.xsl"?>
<ivy-module
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd"
		version="1.3">

	<info organisation="org.apache.jackrabbit" module="com.springsource.org.apache.jackrabbit.extractor"/>

	<configurations>
		<include file="${spring.build.dir}/common/default-ivy-configurations.xml"/>
	</configurations>

	<publications>
		<artifact name="${ant.project.name}"/>
		<artifact name="${ant.project.name}-sources" type="src" ext="jar"/>
		<artifact name="license" type="license" ext="txt"/>
	</publications>

	<dependencies>
		<dependency org="org.apache.poi" name="com.springsource.org.apache.poi" rev="3.0.2.FINAL" conf="compile->compile"/>
		<dependency org="org.apache.xerces" name="com.springsource.org.apache.xerces" rev="2.8.1" conf="compile->compile"/>
		<dependency org="org.cyberneko" name="com.springsource.org.cyberneko.html" rev="0.9.5" conf="compile->compile"/>
		<dependency org="org.pdfbox" name="com.springsource.org.pdfbox" rev="0.7.3" conf="compile->compile"/>
		<dependency org="org.slf4j" name="com.springsource.slf4j.api" rev="1.5.0" conf="compile->compile"/>
		<dependency org="org.textmining" name="com.springsource.org.textmining.extraction" rev="0.4.0" conf="compile->compile"/>
	</dependencies>

</ivy-module>
