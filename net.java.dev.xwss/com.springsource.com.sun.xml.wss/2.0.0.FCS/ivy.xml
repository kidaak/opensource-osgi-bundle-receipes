<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://ivyrep.jayasoft.org/ivy-doc.xsl"?>
<ivy-module
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd"
		version="1.3">

	<info organisation="net.java.dev.xwss" module="com.springsource.com.sun.xml.wss"/>

	<configurations>
		<include file="${spring.build.dir}/common/default-ivy-configurations.xml"/>
	</configurations>

	<publications>
		<artifact name="${ant.project.name}"/>
		<artifact name="license" type="license" ext="txt"/>
	</publications>
	
	<dependencies>
	  <dependency org="javax.mail" name="com.springsource.javax.mail" rev="1.4.1" conf="compile->runtime"/>
	  <dependency org="com.sun.msv" name="com.springsource.com.sun.msv.datatype" rev="0.0.0.20060615" conf="optional->runtime"/>
	  <dependency org="com.sun.xml" name="com.springsource.com.sun.xml.bind" rev="1.0.6" conf="optional->runtime"/>
	  <dependency org="com.sun.msv" name="com.springsource.com.sun.msv.grammar" rev="1.0.6" conf="optional->runtime"/>
	  <dependency org="com.sun.xml" name="com.springsource.com.sun.xml.messaging.saaj" rev="1.3.2" conf="optional->runtime"/>
	  <dependency org="org.relaxng" name="com.springsource.org.relaxng.datatype" rev="0.0.20020414" conf="optional->runtime"/>
	  <dependency org="javax.xml.crypto" name="com.springsource.javax.xml.crypto" rev="1.4.3" conf="optional->runtime"/>
	  <dependency org="javax.xml.rpc" name="com.springsource.javax.xml.rpc" rev="1.1.0" conf="compile->runtime"/>
	</dependencies>

</ivy-module>
