<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://ivyrep.jayasoft.org/ivy-doc.xsl"?>
<ivy-module
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd"
		version="1.3">

	<info organisation="org.powermock" module="com.springsource.org.powermock.mockito"/>

	<configurations>
		<include file="${spring.build.dir}/common/default-ivy-configurations.xml"/>
	</configurations>

	<publications>
		<artifact name="${ant.project.name}"/>
		<artifact name="license" type="license" ext="txt"/>
	</publications>

	<dependencies>
		<dependency org="org.jboss.javassist" name="com.springsource.javassist" rev="3.10.0.GA" conf="compile->runtime"/>
		<dependency org="org.junit" name="com.springsource.org.junit" rev="4.8.1" conf="optional->runtime"/>
		<dependency org="org.mockito" name="com.springsource.org.mockito" rev="1.8.4" conf="compile->runtime"/>
		<dependency org="org.objenesis" name="com.springsource.org.objenesis" rev="1.2.0" conf="compile->runtime"/>
		<dependency org="org.testng" name="com.springsource.org.testng" rev="5.10.0" conf="optional->runtime"/>
	</dependencies>

</ivy-module>

