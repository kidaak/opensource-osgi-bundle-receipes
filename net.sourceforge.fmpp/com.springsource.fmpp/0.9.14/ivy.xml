<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://ivyrep.jayasoft.org/ivy-doc.xsl"?>
<ivy-module
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd"
		version="1.3">

	<info organisation="net.sourceforge.fmpp" module="com.springsource.fmpp"/>

	<configurations>
		<include file="${spring.build.dir}/common/default-ivy-configurations.xml"/>
	</configurations>

	<publications>
		<artifact name="${ant.project.name}"/>
		<artifact name="${ant.project.name}-sources" type="src" ext="jar"/>
		<artifact name="license" type="license" ext="txt"/>
	</publications>

	<dependencies>
		<dependency org="org.beanshell" name="com.springsource.bsh" rev="2.0.0.b4" conf="runtime->runtime"/>
		<dependency org="org.freemarker" name="com.springsource.freemarker" rev="2.3.15" conf="runtime->runtime"/>
		<dependency org="org.apache.oro" name="com.springsource.org.apache.oro" rev="2.0.8" conf="runtime->runtime"/>
		<dependency org="org.apache.ant" name="com.springsource.org.apache.tools.ant" rev="1.7.0" conf="provided->runtime"/>
		<dependency org="org.apache.xml" name="com.springsource.org.apache.xml.resolver" rev="1.2.0" conf="runtime->runtime"/>
	</dependencies>

</ivy-module>
