<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://ivyrep.jayasoft.org/ivy-doc.xsl"?>
<ivy-module
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd"
		version="1.3">

	<info organisation="org.eclipse.osgi" module="org.eclipse.equinox.console.ssh"/>

	<configurations>
		<include file="${spring.build.dir}/common/default-ivy-configurations.xml"/>
	</configurations>

	<publications>
		<artifact name="${ant.project.name}"/>
		<artifact name="${ant.project.name}-sources" type="src" ext="jar"/>
		<artifact name="license" type="license" ext="txt"/>
	</publications>

	<dependencies>
		<dependency org="org.eclipse.osgi" name="org.eclipse.osgi" rev="3.7.0.v20110124-0830" conf="compile->compile"/>
		<dependency org="org.apache.felix" name="org.apache.felix.gogo.runtime" rev="0.8.0.v201105062003" conf="compile->compile"/>
		<dependency org="org.apache.mina" name="org.apache.sshd.core" rev="0.5.0.v201108120515" conf="compile->compile"/>
		<dependency org="org.apache.mina" name="org.apache.mina.core" rev="2.0.2.v201108120515" conf="compile->compile"/>
		<dependency org="org.eclipse.osgi" name="org.eclipse.equinox.console" rev="1.0.0.v20120430-1356" conf="compile->compile"/>
		<dependency org="org.eclipse.osgi" name="org.eclipse.equinox.cm" rev="1.0.300.v20101204" conf="compile->compile"/>
	</dependencies>

</ivy-module>
