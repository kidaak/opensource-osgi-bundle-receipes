<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://ivyrep.jayasoft.org/ivy-doc.xsl"?>
<ivy-module
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd"
		version="1.3">

	<info organisation="org.eclipse.osgi" module="org.eclipse.equinox.p2.touchpoint.natives"/>

	<configurations>
		<include file="${spring.build.dir}/common/default-ivy-configurations.xml"/>
	</configurations>

	<publications>
		<artifact name="${ant.project.name}"/>
		<artifact name="${ant.project.name}-sources" type="src" ext="jar"/>
		<artifact name="license" type="license" ext="txt"/>
	</publications>

	<dependencies>
		<dependency org="org.eclipse.osgi" name="org.eclipse.osgi" rev="3.7.0.v20110613" conf="compile->compile"/>
		<dependency org="org.eclipse.osgi" name="org.eclipse.equinox.common" rev="3.6.100.v20111010-1614" conf="compile->compile"/>
		<dependency org="org.eclipse.osgi" name="org.eclipse.equinox.p2.core" rev="2.1.0.v20110808-1657" conf="compile->compile"/>
		<dependency org="org.eclipse.osgi" name="org.eclipse.equinox.p2.metadata" rev="2.1.0.v20110808-1657" conf="compile->compile"/>
		<dependency org="org.eclipse.osgi" name="org.eclipse.equinox.p2.engine" rev="2.2.0.v20110929-2024" conf="compile->compile"/>
		<dependency org="org.eclipse.osgi" name="org.eclipse.equinox.p2.repository" rev="2.1.0.v20110815-1744" conf="compile->compile"/>
	</dependencies>

</ivy-module>
