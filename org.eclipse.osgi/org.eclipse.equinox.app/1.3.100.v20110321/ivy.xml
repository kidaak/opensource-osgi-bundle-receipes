<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="http://ivyrep.jayasoft.org/ivy-doc.xsl"?>
<ivy-module
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd"
		version="1.3">

	<info organisation="org.eclipse.osgi" module="org.eclipse.equinox.app"/>

	<configurations>
		<include file="${spring.build.dir}/common/default-ivy-configurations.xml"/>
	</configurations>

	<publications>
		<artifact name="${ant.project.name}"/>
		<artifact name="${ant.project.name}-sources" type="src" ext="jar"/>
		<artifact name="license" type="license" ext="txt"/>
	</publications>

	<dependencies>
		<dependency org="org.eclipse.osgi" name="org.eclipse.osgi" rev="3.7.0.v20110124-0830" conf="compile->compile"/>
		<dependency org="org.eclipse.osgi" name="org.eclipse.osgi.services" rev="3.3.0.v20110110" conf="compile->compile"/>
		<dependency org="org.eclipse.osgi" name="org.eclipse.equinox.registry" rev="3.5.100.v20110502" conf="compile->compile"/>
		<dependency org="org.eclipse.osgi" name="org.eclipse.equinox.common" rev="3.6.0.v20110110" conf="compile->compile"/>		
	</dependencies>

</ivy-module>
